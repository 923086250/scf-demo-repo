{
    "serverless-cloud-function-application": {
        "application": {
            "Chinese": {
                "name": "COS 文件備份",
                "description": "此範例使用COS作爲觸發器，在用戶上傳文件到指定bucket時，将該文件複制到另一個用于備份的bucket儲存",
                "attention": "在使用本範例代碼時需要在函數裏修改用戶的appid、secret_id、secret_key、bucket名",
                "readme": {
                    "file": "",
                    "content": ""
                },
                "license": {
                    "file": "",
                    "content": "公開"
                },
                "author": {
                    "name": "Top Cloud 無服務器雲函數團隊"
                }
            },
            "English": {
                "name": "COS_Sync_Backup",
                "description": "This demo will backup the COS bucket object to another bucket when you upload the file",
                "attention": "Please change the appid,secret_id,secret_key,bucket name and region before you use it",
                "readme": {
                    "file": "",// readme file
                    "content": ""//content of readme
                },
                "license": {
                    "file": "",// license file
                    "content": "Open"//content of license
                },
                "author": {
                    "name": "Tencent Cloud Serverless Team"// author
                }
            },
            "input_parameters": {
                "event": "",
                "context": ""
            },
            "output_parameters": {},
            "download_address": "https://github.com/taifucloudyun/scf-demo-repo/tree/master/Php7-Sync_Backup",//demo's git download url demo的git下載連結
            "tags": [
                "Backup",
                "Php7",
                "COS"// The tags only support English and can be multiple, we suggest you to use keywords such as runtime, trigger, environment and so on // 标簽統一爲英文，可編寫多個，建議使用runtime、觸發器、場景等關鍵字，用戶可以通過該關鍵字搜索。前台需要展示，請認真填寫，不支援中文
            ],
            "version": "1.3.3"  // Version number, which identifies the demo version status. // Demo will not to update to the page if the version number unmodified.//版本号，通過版本号标識 demo 升級情況，未修改版本号會導緻 demo 不更新至頁面
        },
        "functions": {
            "name": "COS_Sync_Backup",// Function name which only be in English. // 函數名稱，只支援英文
            "description": "This demo will backup the COS bucket object to another bucket when you upload the file. 此範例使用COS作爲觸發器，在用戶上傳文件到指定bucket時，将該文件複制到另一個用于備份的bucket儲存",
            "handler": "sync_backup.main_handler",
            "memorySize": 128,// Running memory. 運作配置内存
            "timeout": 10,// Running timeout. 運作超時時間
            "runtime": "Php7",//Runtime which users can search demo by this keyword. 運作環境，用戶可以通過該關鍵字搜索["Python2.7", "Python3.6", "Nodejs6.10", "Java8", "LuaCDN", "NodejsCDN", "Php5", "Php7", "Nodejs8.9", "Go1"]
            "Environment": {},// Optional, used to define environment variables. 可選，用于定義環境變量
            "Events": {
                "Type": "COS",
                "Properties": {
                    "Bucket": "mycosbucket",
                    "Events": "cos:ObjectCreated:*",
                    "Filter": "",
                    "Enable": "true"
                }
            },
            // Optional, used define the event source that triggers this function.  可選，用于定義觸發此函數的事件源
            "VpcConfig": {},
            // Optional, used configure cloud function's private network. 可選， 用于配置雲函數訪問 VPC 私有網絡。
            "codeObject": {
                "codeFile": [
                    "sync_backup.php"
                ],
                "CodeUri": [
                    // Code download url which should be same as 'download_address'. 代碼下載網址，和download_address保持一緻
                    "https://github.com/taifucloudyun/scf-demo-repo/tree/master/Php7-Sync_Backup"
                ]
            }
        }
    }
}